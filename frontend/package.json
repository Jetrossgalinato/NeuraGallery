{
  "name": "neura-gallery-frontend",
  "version": "0.1.0",
  "private": true,
  "main": "electron-main.js",
  "dependencies": {
    "axios": "^1.12.2",
    "next": "^15.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^30.0.0",
    "wait-on": "^7.0.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "electron": "npx electron electron-main.js",
    "dev:electron": "concurrently \"next dev\" \"wait-on http://localhost:3000 && electron .\"",
    "dev:desktop": "concurrently \"npm run dev\" \"npm run electron\""
  }
}
